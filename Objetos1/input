{
    classe Contador {
        int valor = 10;
        proc alteraValor() {
            this.valor := 9
        },
        proc print() {
            write(this.valor)
        }
    };

    suite MeuTeste {
        int valor = 10;
        test verificaValor() {
            assert(this.valor,10);
            this.valor := 9
        }
    };

    {
        Contador c := new Contador;
        write("Teste do write");
        c.print()
    }
}