  {
      classe Contador {
          int valor = 10;
          proc alteraValor() {
            this.valor := 9
          },
          proc print() {
            this.alteraValor();
            assert(this.valor, 9);
            write(this.valor)
          }
      }     
      ;

      {
        Contador c := new Contador
        ;
	write("Teste do write");
        c.print()
     }

   }